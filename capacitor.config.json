{
  "appId": "com.sergiolara.remotecontroller",
  "appName": "Remote Controller",
  "webDir": "dist",
  "server": {
    "androidScheme": "http",
    "allowNavigation": ["*"],
    "cleartext": true
  },
  "android": {
    "allowMixedContent": true,
    "usesCleartextTraffic": true,
    "webSecurity": false
  },
  "plugins": {
    "CapacitorHttp": {
      "enabled": true
    },
    "LocalNotifications": {
      "smallIcon": "ic_stat_icon_config_sample",
      "iconColor": "#488AFF"
    }
  }
}
